{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/clerk",
      "dest": "server.js",
      "methods": ["POST"]
    },
    {
      "src": "/stripe",
      "dest": "server.js",
      "methods": ["POST"]
    },
    {
      "src": "/",
      "dest": "server.js",
      "methods": ["GET"]
    }
  ]
}